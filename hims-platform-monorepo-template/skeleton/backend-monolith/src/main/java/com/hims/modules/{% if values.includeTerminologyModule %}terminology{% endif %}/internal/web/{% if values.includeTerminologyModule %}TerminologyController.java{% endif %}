package com.hims.modules.terminology.internal.web;

import com.hims.modules.terminology.api.spi.TerminologyServiceProvider;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
@RequestMapping("/api/v1/terminology")
public class TerminologyController {
    private final TerminologyServiceProvider service;
    public TerminologyController(TerminologyServiceProvider service) { this.service = service; }
    @GetMapping("/health")
    public String health() { return "terminology module is healthy"; }
}

