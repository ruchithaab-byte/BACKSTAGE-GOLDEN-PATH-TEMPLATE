package com.hims.modules.warehouse.internal.web;

import com.hims.modules.warehouse.api.spi.WarehouseServiceProvider;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
@RequestMapping("/api/v1/warehouse")
public class WarehouseController {
    private final WarehouseServiceProvider service;
    public WarehouseController(WarehouseServiceProvider service) { this.service = service; }
    @GetMapping("/health")
    public String health() { return "warehouse module is healthy"; }
}

