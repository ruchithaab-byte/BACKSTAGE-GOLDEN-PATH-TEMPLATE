package com.hims.modules.integration.internal.web;

import com.hims.modules.integration.api.spi.IntegrationServiceProvider;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
@RequestMapping("/api/v1/integration")
public class IntegrationController {
    private final IntegrationServiceProvider service;
    public IntegrationController(IntegrationServiceProvider service) { this.service = service; }
    @GetMapping("/health")
    public String health() { return "integration module is healthy"; }
}

